<DefaultWindow xmlns="https://spacestation14.io"
            MinSize="400 100" Access="Public">
    <BoxContainer Orientation="Vertical">
        <BoxContainer Orientation="Horizontal">
            <Label Text="Transmitting To" Margin="0 0 10 0" StyleClasses="LabelKeyText" />
            <Control HorizontalExpand="True"/>
            <OptionButton Name="PossibleOutputs" Access="Public"/>
        </BoxContainer>
        <BoxContainer Orientation="Horizontal">
            <Label Text="Receiving From" Margin="0 0 10 0" StyleClasses="LabelKeyText" />
            <Control HorizontalExpand="True"/>
            <OptionButton Name="PossibleInputs" Access="Public"/>
        </BoxContainer>
    </BoxContainer>
</DefaultWindow>
